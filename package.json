{
  "name": "activate-components",
  "version": "0.0.1",
  "description": "base components for hte Activate project",
  "author": "ayanes",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "homepage": "https://github.com/AlejandroYanes/activate-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlejandroYanes/activate-components.git"
  },
  "bugs": {
    "url": "https://github.com/AlejandroYanes/activate-components/issues"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "npm run build:ems && npm run build:cjs",
    "build:ems": "ttsc",
    "build:cjs": "ttsc --module commonjs --outDir lib/cjs",
    "install:husky": "husky install",
    "lint": "eslint --ext=jsx,ts,tsx 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --ext=jsx,ts,tsx 'src/**/*.{ts,tsx}' --fix",
    "test": "jest --watch=false",
    "test:watch": "jest --watch",
    "test:all": "jest --watchAll=false",
    "test:ci": "jest --watchAll=false --ci --coverage --reporters=default --reporters=jest-junit --coverageReporters=cobertura"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.1"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "date-fns": "^2.16.1",
    "framer-motion": "^4.1.17",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/enzyme": "^3.10.9",
    "@types/expect": "^24.3.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autosize": "^4.0.2",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.0.8",
    "framer-motion": "^4.1.17",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-expect": "^0.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "rxjs": "^7.4.0",
    "styled-components": "^5.3.1",
    "ts-jest": "^27.0.6",
    "ttypescript": "^1.5.12",
    "typescript": "4.4.4"
  }
}
